---
import { getNav, keyDocuments } from "@/utils/sanity";
import "../styles/global.css";
import { Navigation } from "./Navigation";
import { MobileNavigation } from "./MobileNavigation";
type Props = {};

const navContent = await getNav(keyDocuments.MAIN_NAV);
const {} = Astro.props;
---

<header class="flex items-center justify-between p-4">
  <a href="/" class="text-lg font-extrabold">
    <h1>Max Maciag</h1>
  </a>
  <Navigation client:only="react" navigationContent={navContent} />
  <MobileNavigation client:only="react" navigationContent={navContent} />
</header>
