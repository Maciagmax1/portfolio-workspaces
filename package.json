{
  "name": "portfolio",
  "description": "Minimal Astro site with Sanity Studio",
  "homepage": "https://max-maciag.sanity.studio",
  "type": "module",
  "scripts": {
    "app:build": "pnpm --filter=./astro-app run build",
    "app:dev": "pnpm --filter=./astro-app run dev",
    "dev": "concurrently \"pnpm --filter=./astro-app run dev\" \"pnpm --filter=./studio run dev\"",
    "lint": "npx eslint .",
    "prettier": "npx prettier --write .",
    "studio:build": "pnpm --filter=./studio run build",
    "studio:deploy": "cd studio & npx sanity deploy",
    "studio:dev": "pnpm --filter=./studio run dev"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.18",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  }
}
